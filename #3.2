int main() {
	setlocale(LC_CTYPE, "RUS");
	float a, min, max, res;
	int i = 2, n, a1, a2; //инициализируем i как int; инициализируем n, a1, a2
	cout << "a1= , a2=" << endl;
	cin >> a1 >> a2; // Заменяем запятые на "<<"
	if (a1 > a2)
	{
		//Добавялем ";"
		max = a1; //Меняем местами
		min = a2; //Меняем местами
	}//Убираем ;
	else
	{ // Добавляем скобки
		max = a2; //Меняем местами
		min = a1; //Меняем местами
	}
