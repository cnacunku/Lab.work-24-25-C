#include <iostream>
using namespace std;
//3.1
int main() {
    setlocale(LC_CTYPE, "RUS");
    int n, j = 1;
    cout << "Введите значение для n: " << endl;
    cin >> n;
    cout << "Делители числа " << n << ":" << endl;

    while (j <= n) {
        if (n % j == 0) {
            cout << j << endl;
        }
        j++;
    }

    return 0;
}
